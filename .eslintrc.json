{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
        
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "indent": ["error", 2]
    },
    "settings": {
        "react": {
          "version": "detect"
        }
      }
}
